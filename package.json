{
  "name": "juhe-tools-web",
  "private": true,
  "version": "0.0.4",
  "description": "ðŸŒˆðŸŽˆâœ¨ðŸŽ‰ðŸŽŠ An image manager application based on electron, vue3 and vite.",
  "main": "dist-electron/index.js",
  "scripts": {
    "dev": "vite",
    "build:web": "vite build",
    "build": "rimraf dist && npx yarn autoclean -I && npx yarn autoclean -F && vite build && npx electron-builder",
    "build:win": "rimraf dist && npx yarn autoclean -I && npx yarn autoclean -F && vite build && npx electron-builder --win",
    "build:mac": "rimraf dist && npx yarn autoclean -I && npx yarn autoclean -F && vite build && npx electron-builder --mac",
    "build:linux": "rimraf dist && npx yarn autoclean -I && npx yarn autoclean -F && vite build && npx electron-builder --linux",
    "preview": "vite preview",
    "prettier": "npx prettier --config .prettierrc.js --write ./src/**/*.vue && npx prettier --config .prettierrc.js --write ./src/**/*.js"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "ant-design-vue": "^3.2.16",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "pinia": "^2.0.33",
    "uuid": "^9.0.0",
    "vue": "^3.2.47",
    "vue-router": "4",
    "vue-virtual-list-v3": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@vitejs/plugin-vue": "^4.1.0",
    "electron": "^24.1.1",
    "electron-devtools-installer": "^3.2.0",
    "npm-check-updates": "^16.8.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "semicolons": "^1.0.2",
    "vite": "^4.2.1",
    "vite-plugin-electron": "^0.11.1"
  },
  "build": {
    "appId": "com.juhetools.app",
    "productName": "JuheTools",
    "asar": true,
    "copyright": "Copyright Â© 2023 JuheTools",
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist",
      "dist-electron",
      "!node_modules/asar/**/*"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "electron/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false,
      "deleteAppDataOnUninstall": false,
      "createStartMenuShortcut": true,
      "createDesktopShortcut": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080"
      }
    ],
    "releaseInfo": {
      "releaseNotes": "æš‚æ—¶æ²¡æœ‰å¡«å†™"
    }
  }
}
